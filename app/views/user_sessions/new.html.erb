<h1>User Log in</h1>
<%= form_with(scope: :session, url: user_sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
  <%= link_to 'user sign up', new_user_path %>
<% end %>