<h1>Owner Sign up</h1>

<%= form_with(model: @owner, local: true) do |f| %>  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create my account" %>
  <%= link_to '', new_user_session_path %>
<% end %>
<%= link_to 'userはコチラ', new_user_session_path %>
<%= link_to 'stylistはコチラ', new_stylist_session_path %>
