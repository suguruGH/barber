<h1>Owner Log in </h1>
<%= form_with(scope: :session, url: owner_sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
  <%= link_to 'owner sign up', new_owner_path %>
<% end %>
